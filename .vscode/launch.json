    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch local debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/MapleSDK/build/Release-linux/airplaydemo",
            "args": [
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make-x86-target"
        },
        {
            "name": "Launch remote debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/MapleSDK/build/Release-linux/airplaydemo",
            "args": [],
            "miDebuggerServerAddress": "${env:MOZART_REMOTE_DEBUG_IP}:9091",
            "stopAtEntry": true,
            "targetArchitecture": "arm",
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set  remmote sysroot to enable loading shared library symbold",
                    "text": "set sysroot remote:/",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "prepare-remote-debug",
            "miDebuggerPath": "${env:MOZART_SDK_GDB_PATH}"
        }
    ]
}